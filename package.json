{
  "name": "@yokogawaelectric/xy-designsystem",
  "version": "3.2.0",
  "description": "Yokogawa Design System components extend [Angular Material](https://material.angular.io/)",
  "author": "Takayuki Takeuchi",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/yokogawaelectric"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/yokogawaelectric/xy-designsystem.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build xy-designsystem",
    "build-app": "ng build --base-href=/XYDS-SampleApp/xy-designsystem-app",
    "watch": "ng build --watch --configuration development xy-designsystem",
    "test": "ng test",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "output-storybook": "npm run docs:json && build-storybook -o ./docs/storybook",
    "predeploy": "build-storybook",
    "deploy": "gh-pages -d docs/storybook",
    "analyze": "ng build --prod && source-map-explorer dist/main.*.js"
  },
  "dependencies": {
    "@adminjs/typeorm": "^2.0.0",
    "@angular/animations": "^16.2.1",
    "@angular/common": "^16.2.1",
    "@angular/compiler": "^16.2.1",
    "@angular/core": "^16.2.1",
    "@angular/forms": "^16.2.1",
    "@angular/material": "^16.2.1",
    "@angular/material-moment-adapter": "^16.2.1",
    "@angular/platform-browser": "^16.2.1",
    "@angular/platform-browser-dynamic": "^16.2.1",
    "@angular/router": "^16.2.1",
    "@mdx-js/react": "^1.6.22",
    "@nestjs/core": "^7.6.15",
    "@types/react-dom": "^17.0.19",
    "@types/webpack": "^5.28.0",
    "@yokogawaelectric/xy-designsystem": "^2.1.5",
    "momen": "^0.0.1-security",
    "moment": "^2.29.4",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "typeface-noto-sans": "^1.1.13",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1602.0",
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular/cdk": "^16.2.1",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.2.1",
    "@babel/core": "^7.17.5",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/angular": "^6.4.13",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.4.13",
    "@storybook/testing-library": "^0.0.9",
    "@types/jasmine": "^3.10.0",
    "@types/node": "^12.11.1",
    "@types/react": "^17.0.55",
    "babel-loader": "^8.2.3",
    "can-npm-publish": "1.3.6",
    "gh-pages": "^3.2.3",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^16.2.0",
    "react": "^17.0.2",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^17.0.2",
    "source-map-explorer": "^2.5.3",
    "storybook-addon-themes": "^6.1.0",
    "typescript": "^5.1.6"
  }
